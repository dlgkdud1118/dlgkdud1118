package hitoride;

import java.util.Random;

public class SwitchTest03 {

	public static void main(String[] args) {

		Random random = new Random();

		int time = random.nextInt(4) + 8;

		switch (time) {
		case 8:
			System.out.println("출근할 시간이다.");
		case 9:
			System.out.println("회의할 시간이다.");
		case 10:
			System.out.println("업무할 시간이다.");
		case 11:
			System.out.println("외근을 나가야 할까?");
		}
	}

}
