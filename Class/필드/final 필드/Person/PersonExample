package dlgkdud;

public class PersonExample {
	public static void main(String[] args) {

		Person p1 = new Person("12345-6789", "이하영");

		System.out.println(p1.nation);
		System.out.println(p1.ssn);
		System.out.println(p1.name);

		// p1.nation = "usa";
		// p1.ssn = "1234-5789";
		p1.name = "dlgkdud";
	}
}
